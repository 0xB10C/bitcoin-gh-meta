      "author_association" : "MEMBER",
      "body" : "Maybe list the address types here.",
      "commit_id" : "b6b99e0ac55d5078df3a665323fd9e5310fc2031",
      "created_at" : "2022-09-02T11:44:49Z",
      "diff_hunk" : "@@ -754,6 +754,159 @@ static RPCHelpMan migratewallet()\n     };\n }\n \n+static RPCHelpMan setdescriptorhdkey()\n+{\n+    return RPCHelpMan{\"setdescriptorhdkey\",\n+        \"Set or generate a new HD key for this descriptor wallet. The key will only be used for new automatically generated descriptors created with 'createwalletdescriptor'.\"\n+        \"No new descriptors will be created by using this RPC. It only prepares the wallet to do so.\"\n+        \"\\nNote that you will need to MAKE A NEW BACKUP of your wallet after setting the HD wallet key.\" +\n+        HELP_REQUIRING_PASSPHRASE,\n+        {{\"key\", RPCArg::Type::STR, RPCArg::DefaultHint{\"random seed\"}, \"A BIP 32 extended private key (xprv) to set as the wallet's HD Key, or a BIP 32 HD seed encoded as a WIF private key or hex string.\"}},\n+        RPCResult{RPCResult::Type::NONE, \"\", \"\"},\n+        RPCExamples{\n+            HelpExampleCli(\"setdescriptorhdkey\", \"\")\n+            + HelpExampleRpc(\"setdescriptorhdkey\", \"\")\n+        },\n+    [&](const RPCHelpMan& self, const JSONRPCRequest& request) -> UniValue\n+{\n+    std::shared_ptr<CWallet> const pwallet = GetWalletForJSONRPCRequest(request);\n+    if (!pwallet) return UniValue::VNULL;\n+\n+    if (pwallet->IsWalletFlagSet(WALLET_FLAG_DISABLE_PRIVATE_KEYS)) {\n+        throw JSONRPCError(RPC_WALLET_ERROR, \"Cannot set a HD key to a wallet with private keys disabled\");\n+    }\n+\n+    //  Make sure wallet is a descriptor wallet\n+    if (!pwallet->IsWalletFlagSet(WALLET_FLAG_DESCRIPTORS)) {\n+        throw JSONRPCError(RPC_WALLET_ERROR, \"setdescriptorhdkey is not available for non-descriptor wallets\");\n+    }\n+    EnsureWalletIsUnlocked(*pwallet);\n+\n+    KeyManager& keyman = pwallet->GetKeyManager();\n+    if (request.params[0].isNull()) {\n+        // Generate new key if none is specified\n+        keyman.GenerateAndSetHDKey();\n+    } else {\n+        // First try decoding as xprv\n+        std::string key_str = request.params[0].get_str();\n+        CExtKey master_key = DecodeExtKey(key_str);\n+        if (!master_key.key.IsValid()) {\n+            // The following are for handling the key as a seed\n+            std::vector<unsigned char> seed_bytes;\n+            // Next try decoding as WIF\n+            CKey key = DecodeSecret(key_str);\n+            seed_bytes.assign(key.begin(), key.end());\n+            if (!key.IsValid()) {\n+                // Lastly try decoding hex\n+                if (!IsHex(key_str)) {\n+                    throw JSONRPCError(RPC_INVALID_ADDRESS_OR_KEY, \"Key must be a BIP 32 extended private key, or the BIP 32 seed as a WIF private key or a hex string.\");\n+                }\n+                seed_bytes = ParseHex(key_str);\n+            }\n+            master_key.SetSeed(MakeByteSpan(seed_bytes));\n+        }\n+        CExtPubKey master_xpub = master_key.Neuter();\n+\n+        // Write the key\n+        WalletBatch batch(pwallet->GetDatabase());\n+        bool ok = keyman.AddHDKey(batch, master_key, master_xpub);\n+        assert(ok);\n+        keyman.SetActiveHDKey(master_xpub);\n+    }\n+\n+    return UniValue::VNULL;\n+}\n+    };\n+}\n+\n+static RPCHelpMan createwalletdescriptor()\n+{\n+    return RPCHelpMan{\"createwalletdescriptor\",\n+        \"Creates the wallet's descriptor for the given address type. \"\n+        \"Only works on wallets that contain automatically generated descriptors. \"\n+        \"The address type must be one that the wallet does not already have a descriptor for.\"\n+        + HELP_REQUIRING_PASSPHRASE,\n+        {\n+            {\"address_type\", RPCArg::Type::STR, RPCArg::Optional::NO, \"The address type the descriptor will produce\"},",
