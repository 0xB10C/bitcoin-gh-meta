      "diff_hunk" : "@@ -0,0 +1,52 @@\n+#!/usr/bin/env python3\n+# Copyright (c) 2020 The Bitcoin Core developers\n+# Distributed under the MIT software license, see the accompanying\n+# file COPYING or http://www.opensource.org/licenses/mit-license.php.\n+\"\"\"Test that listtransactions shows whether a transaction is in the mempool.\"\"\"\n+from test_framework.test_framework import BitcoinTestFramework\n+from test_framework.util import assert_equal\n+\n+class WalletShowConflictedInactiveTransactionsTest(BitcoinTestFramework):\n+    def set_test_params(self):\n+        self.num_nodes = 1\n+\n+    def skip_test_if_missing_module(self):\n+        self.skip_if_no_wallet()\n+\n+    def create_and_send_transaction(self, utxo, address, amt, feeRate):\n+        psbtx = self.nodes[0].walletcreatefundedpsbt([{\"txid\": utxo['txid'], \"vout\": utxo['vout']}],\n+                                                      {address: amt},\n+                                                      0,\n+                                                      {\"replaceable\":True, \"feeRate\":feeRate})['psbt']\n+        signed_tx = self.nodes[0].walletprocesspsbt(psbtx)['psbt']\n+        final_tx = self.nodes[0].finalizepsbt(signed_tx)['hex']\n+        return self.nodes[0].sendrawtransaction(final_tx)\n+\n+    def test_listtransactions_display_inmempool(self):\n+        utxo = self.nodes[0].listunspent()[0]\n+        address = self.nodes[0].getnewaddress()\n+\n+        tx1_id = self.create_and_send_transaction(utxo, address, 10, 0.001)\n+\n+        new_txs = self.nodes[0].listtransactions(count=2)\n+        for tx in new_txs:\n+            assert_equal(tx['txid'], tx1_id)\n+            assert_equal(tx['inmempool'], True)\n+\n+        tx2_id = self.create_and_send_transaction(utxo, address, 10, 0.002)\n+\n+        new_txs = self.nodes[0].listtransactions(count=4)",
